generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Usuarios {
  IDUsuario Int          @id @default(autoincrement())
  nome      String
  treinos   Exercicios[]
}

model Exercicios {
  IDExercicio       Int      @id @default(autoincrement())
  nomeExercicio     String
  detalhesExercicio String
  IDUsuario         Int
  usuario           Usuarios @relation(fields: [IDUsuario], references: [IDUsuario])
}
